{
  "name": "vitepress-auto-nav-sidebar",
  "version": "0.1.0",
  "description": "自动生成vitepress导航栏和侧边栏",
  "keywords": [
    "vitepress-plugin",
    "vitepress"
  ],
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "publish": "semantic-release",
    "lint": "eslint --fix --ext .ts src",
    "prepare": "husky install"
  },
  "author": "merlin218",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Merlin218/vitepress-auto-nav-sidebar.git"
  },
  "homepage": "https://github.com/Merlin218/vitepress-auto-nav-sidebar/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/Merlin218/vitepress-auto-nav-sidebar/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@swc/core": "^1.6.13",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "semantic-release": "^24.0.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "tsup": "^8.1.0",
    "typescript": "^4.6.2",
    "vitepress": "1.0.0-alpha.2"
  },
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  }
}
